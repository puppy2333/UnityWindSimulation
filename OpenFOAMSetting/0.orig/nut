FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -1 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    upSlipBnd
    {
        type            symmetryPlane;
    }
    downSolidBnd
    {
        type            nutUWallFunction;
        value           uniform 0;
    }
    frontSlipBnd
    {
        type            symmetryPlane;
    }
    backSlipBnd
    {
        type            symmetryPlane;
    }
    solidBnd
    {
        type            nutUWallFunction;
        value           uniform 0;
    }
}


